#include <stdio.h>
#include <stlib.h>
#include "heap.c"

void size_array(int v[]) {
    return sizeof(v)/sizeof(v[0]);
}

void print_array(int v[]) {
    for(int i = 0; i < size_array(v); i++) printf("%d ", v[i]);
}

void teste_constroi() {
    int v[] = {1,2,3,4,5,6,7,8,9};
    constroi_heap(v, size_array(v));
    print_array(v);
}

int main() {
    teste_constroi();

    return EXIT_SUCCESS;
}
